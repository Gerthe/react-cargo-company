{
  "name": "react-cargo-company",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.19.2",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "concurrently": "8.2.2",
    "dayjs": "^1.11.13",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mysql2": "^3.11.3",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.26.2",
    "react-scripts": "^5.0.1",
    "react-tiktok": "^1.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postbuild-snap": "react-snap",
    "start": "concurrently \"npm run start-react\" \"npm run start-backend\"",
    "start-react": "react-scripts start",
    "start-backend": "nodemon server/index.js",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.11.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.36.1",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "nodemon": "^3.1.7",
    "prettier": "3.3.3",
    "react-snap": "^1.23.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
